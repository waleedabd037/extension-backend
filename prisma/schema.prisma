generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  user_id              String  @id
  trial_start          BigInt
  trial_ended_logged   Boolean @default(false)
  license              Boolean @default(false)
  license_activated_at BigInt?
  license_ended_logged Boolean @default(false)
}
